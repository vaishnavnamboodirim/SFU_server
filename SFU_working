function createViewer(type) {
  if (type === "default" || type === "") {
    let viewer = new Viewer(document.getElementById("canvas-view"));
    return viewer;
  }

  if (type === "h264" || type === "avc") {
    //let viewer = new VideoPlayer(document.getElementById("video-view"));
    //return viewer;
    return null;
  }
}
