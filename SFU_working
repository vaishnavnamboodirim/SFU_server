function subscribeEvents() {
  // IME parent input window
  ime_elm = document.getElementById("input-ime");
  imeOpened = false;

  viewer = getViewerElement(type);

  // Prevent showing context menu of canvas
  viewer.addEventListener("contextmenu", function (event) {
    DisableDefault(event);
  });
  
  function getViewerElement(type) {
  let elmnt = null;
  if ("default" === type || "" === type) {
    elmnt = document.getElementById("canvas-view");
  }
  return elmnt;
}
